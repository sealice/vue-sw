    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "vue/require-v-for-key": "warn"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
